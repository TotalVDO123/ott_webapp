{
  "name": "@jwp/access-bridge",
  "version": "1.0.0",
  "main": "build/main.js",
  "private": true,
  "author": "JW Player",
  "type": "module",
  "scripts": {
    "build": "tsc && [ -f .env ] && cp .env ./build/.env || true && [ -f .env.local ] && cp .env.local ./build/.env.local || true",
    "start": "node build/src/main.js",
    "prestart": "yarn build",
    "pretest": "yarn build",
    "test": "node --test",
    "lint:ts": "tsc --pretty --noEmit -p ./"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "eslint-config-jwp": "*",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "@jwp/ott-common": "*"
  }
}
